        </a>
      {% endif %}
    </div>
  </div>
</schnappix-header-{{ ai_gen_id }}>

<script>
  (function() {
    class SchnappixHeader{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
      }

      connectedCallback() {
        this.toggle = this.querySelector('.ai-schnappix-header__mobile-toggle-{{ ai_gen_id }}');
        this.nav = this.querySelector('.ai-schnappix-header__nav-{{ ai_gen_id }}');
        this.menuLinks = this.querySelectorAll('.ai-schnappix-header__menu-link-{{ ai_gen_id }}');

        if (this.toggle && this.nav) {
          this.toggle.addEventListener('click', () => this.toggleMenu());
          this.menuLinks.forEach(link => {
            link.addEventListener('click', () => this.closeMenu());
          });
        }
      }

      toggleMenu() {
        const isActive = this.nav.classList.contains('active');
        this.nav.classList.toggle('active');
        this.toggle.classList.toggle('active');
        this.toggle.setAttribute('aria-expanded', !isActive);
      }

      closeMenu() {
        this.nav.classList.remove('active');
        this.toggle.classList.remove('active');
        this.toggle.setAttribute('aria-expanded', 'false');
      }
    }

    customElements.define('schnappix-header-{{ ai_gen_id }}', SchnappixHeader{{ ai_gen_id }});
  })();
</script>


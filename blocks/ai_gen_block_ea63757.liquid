{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-schnappix-announcement-{{ ai_gen_id }} {
    width: 100%;
    background-color: {{ block.settings.announcement_bg_color }};
    padding: {{ block.settings.announcement_padding }}px 0;
    text-align: center;
    color: {{ block.settings.announcement_text_color }};
    font-size: {{ block.settings.announcement_size }}px;
  }

  .ai-schnappix-header-{{ ai_gen_id }} {
    background-color: {{ block.settings.background_color }};
    border-bottom: {{ block.settings.border_thickness }}px solid {{ block.settings.border_color }};
    position: sticky;
    top: 0;
    z-index: 100;
    width: 100%;
  }

  .ai-schnappix-header__container-{{ ai_gen_id }} {
    max-width: {{ block.settings.max_width }}px;
    margin: 0 auto;
    padding: {{ block.settings.vertical_padding }}px {{ block.settings.horizontal_padding }}px;
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 40px;
  }

  .ai-schnappix-header__logo-{{ ai_gen_id }} {
    flex-shrink: 0;
  }

  .ai-schnappix-header__logo-link-{{ ai_gen_id }} {
    display: inline-block;
    text-decoration: none;
    color: {{ block.settings.logo_color }};
    font-family: {{ block.settings.logo_font.family }}, {{ block.settings.logo_font.fallback_families }};
    font-weight: {{ block.settings.logo_font.weight }};
    font-size: {{ block.settings.logo_size }}px;
    line-height: 1.2;
    letter-spacing: 0.5px;
  }

  .ai-schnappix-header__nav-{{ ai_gen_id }} {
    flex-grow: 1;
    display: flex;
    justify-content: center;
  }

  .ai-schnappix-header__menu-{{ ai_gen_id }} {
    display: flex;
    gap: {{ block.settings.nav_spacing }}px;
    list-style: none;
    margin: 0;
    padding: 0;
    align-items: center;
  }

  .ai-schnappix-header__menu-item-{{ ai_gen_id }} {
    margin: 0;
  }

  .ai-schnappix-header__menu-link-{{ ai_gen_id }} {
    color: {{ block.settings.nav_color }};
    text-decoration: none;
    font-family: {{ block.settings.nav_font.family }}, {{ block.settings.nav_font.fallback_families }};
    font-weight: {{ block.settings.nav_font.weight }};
    font-size: {{ block.settings.nav_size }}px;
    transition: color 0.3s ease;
    display: block;
    padding: 8px 0;
  }

  .ai-schnappix-header__menu-link-{{ ai_gen_id }}:hover {
    color: {{ block.settings.nav_hover_color }};
  }

  .ai-schnappix-header__actions-{{ ai_gen_id }} {
    display: flex;
    gap: {{ block.settings.cta_spacing }}px;
    align-items: center;
    flex-shrink: 0;
  }

  .ai-schnappix-header__cta-{{ ai_gen_id }} {
    display: inline-block;
    padding: {{ block.settings.cta_padding_vertical }}px {{ block.settings.cta_padding_horizontal }}px;
    background-color: {{ block.settings.cta_bg_color }};
    color: {{ block.settings.cta_text_color }};
    text-decoration: none;
    border-radius: {{ block.settings.cta_border_radius }}px;
    font-family: {{ block.settings.cta_font.family }}, {{ block.settings.cta_font.fallback_families }};
    font-weight: {{ block.settings.cta_font.weight }};
    font-size: {{ block.settings.cta_size }}px;
    transition: background-color 0.3s ease, color 0.3s ease;
    white-space: nowrap;
  }

  .ai-schnappix-header__cta-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.cta_hover_bg_color }};
    color: {{ block.settings.cta_hover_text_color }};
  }

  .ai-schnappix-header__cart-{{ ai_gen_id }} {
    color: {{ block.settings.cart_color }};
    text-decoration: none;
    font-family: {{ block.settings.nav_font.family }}, {{ block.settings.nav_font.fallback_families }};
    font-weight: {{ block.settings.nav_font.weight }};
    font-size: {{ block.settings.nav_size }}px;
    transition: color 0.3s ease;
    white-space: nowrap;
  }

  .ai-schnappix-header__cart-{{ ai_gen_id }}:hover {
    color: {{ block.settings.nav_hover_color }};
  }

  .ai-schnappix-header__mobile-toggle-{{ ai_gen_id }} {
    display: none;
    background: none;
    border: none;
    color: {{ block.settings.nav_color }};
    cursor: pointer;
    padding: 8px;
    flex-direction: column;
    gap: 4px;
  }

  .ai-schnappix-header__mobile-toggle-{{ ai_gen_id }} span {
    width: 24px;
    height: 2px;
    background-color: currentColor;
    transition: all 0.3s ease;
  }

  .ai-schnappix-header__mobile-toggle-{{ ai_gen_id }}.active span:nth-child(1) {
    transform: rotate(45deg) translate(5px, 5px);
  }

  .ai-schnappix-header__mobile-toggle-{{ ai_gen_id }}.active span:nth-child(2) {
    opacity: 0;
  }

  .ai-schnappix-header__mobile-toggle-{{ ai_gen_id }}.active span:nth-child(3) {
    transform: rotate(-45deg) translate(6px, -6px);
  }

  @media screen and (max-width: 990px) {
    .ai-schnappix-header__nav-{{ ai_gen_id }} {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100vh;
      background-color: {{ block.settings.background_color }};
      flex-direction: column;
      justify-content: flex-start;
      padding: 80px 20px 20px;
      transform: translateX(-100%);
      transition: transform 0.3s ease;
      z-index: 99;
    }

    .ai-schnappix-header__nav-{{ ai_gen_id }}.active {
      transform: translateX(0);
    }

    .ai-schnappix-header__menu-{{ ai_gen_id }} {
      flex-direction: column;
      gap: 20px;
      width: 100%;
    }

    .ai-schnappix-header__menu-link-{{ ai_gen_id }} {
      font-size: 20px;
      padding: 12px 0;
    }

    .ai-schnappix-header__mobile-toggle-{{ ai_gen_id }} {
      display: flex;
    }

    .ai-schnappix-header__actions-{{ ai_gen_id }} {
      gap: 12px;
    }

    .ai-schnappix-header__cta-{{ ai_gen_id }} {
      padding: 10px 16px;
      font-size: 14px;
    }

    .ai-schnappix-header__cart-{{ ai_gen_id }} {
      font-size: 14px;
    }
  }

  @media screen and (max-width: 749px) {
    .ai-schnappix-header__container-{{ ai_gen_id }} {
      padding: 16px 20px;
    }

    .ai-schnappix-header__logo-link-{{ ai_gen_id }} {
      font-size: {{ block.settings.logo_size | times: 0.8 }}px;
    }

    .ai-schnappix-header__cta-{{ ai_gen_id }} {
      display: none;
    }

    .ai-schnappix-announcement-{{ ai_gen_id }} {
      font-size: {{ block.settings.announcement_size | times: 0.9 }}px;
      padding: {{ block.settings.announcement_padding | times: 0.8 }}px 16px;
    }
  }
{% endstyle %}

